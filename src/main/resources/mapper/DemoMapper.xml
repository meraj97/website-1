<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wgn.website.mapper.DemoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.wgn.website.entity.DemoEntity">
    <result column="id" property="id" />
    <result column="tenant_id" property="tenantId" />
    <result column="create_by_id" property="createById" />
    <result column="create_by_name" property="createByName" />
    <result column="create_time" property="createTime" />
    <result column="modified_by_id" property="modifiedById" />
    <result column="modified_by_name" property="modifiedByName" />
    <result column="modified_time" property="modifiedTime" />
    <result column="remark" property="remark" />
    <result column="logic_del" property="logicDel" />
    <result column="version" property="version" />
        <result column="demo_title" property="demoTitle" />
        <result column="demo_author" property="demoAuthor" />
        <result column="demo_link" property="demoLink" />
        <result column="demo_img" property="demoImg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        tenant_id,
        create_by_id,
        create_by_name,
        create_time,
        modified_by_id,
        modified_by_name,
        modified_time,
        remark,
        logic_del,
        version,
        demo_title, demo_author, demo_link, demo_img
    </sql>

</mapper>
