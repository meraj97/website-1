<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wgn.website.sys.mapper.DocMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.wgn.website.sys.entity.DocEntity">
    <result column="id" property="id" />
    <result column="tenant_id" property="tenantId" />
    <result column="create_by_id" property="createById" />
    <result column="create_by_name" property="createByName" />
    <result column="create_time" property="createTime" />
    <result column="modified_by_id" property="modifiedById" />
    <result column="modified_by_name" property="modifiedByName" />
    <result column="modified_time" property="modifiedTime" />
    <result column="remark" property="remark" />
    <result column="logic_del" property="logicDel" />
    <result column="version" property="version" />
        <result column="doc_title" property="docTitle" />
        <result column="doc_price" property="docPrice" />
        <result column="doc_link" property="docLink" />
        <result column="doc_img" property="docImg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        tenant_id,
        create_by_id,
        create_by_name,
        create_time,
        modified_by_id,
        modified_by_name,
        modified_time,
        remark,
        logic_del,
        version,
        doc_title, doc_price, doc_link, doc_img
    </sql>

</mapper>
